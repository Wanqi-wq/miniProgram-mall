/* pages/profile/childComps/profile-list-view/profile-list-view.wxss */
.list {
    margin-top: 24rpx;
    background-color: rgba(100, 100, 100, .1);
    font-size: 30rpx;
    color: #333;
    padding-top: 30rpx;
  }
  .list .item {
    height: 88rpx;
    line-height: 88rpx;
    position: relative;
    background: #fff;
    border-bottom: 1px solid rgba(100, 100, 100, .1);
  }
.item .icon {
    margin-top: 10px;
    position: absolute;
    left: 16px;
    top: -7px;
  }
  .icon .img {
    width: 36rpx;
    height: 36rpx;
  }
  .item .text {
    margin-left: 40px;
    padding-left: 5px;
  }
  .item:last-of-type .info {
    border-bottom: none;
  }
  .orderlist, .servicelist{
    margin-top: 24rpx;
  }